{
  "name": "tenant-ai-test-generator",
  "version": "2.0.0",
  "description": "Full-stack Tenant Management Portal with AI-powered testing using Cypress and Playwright",
  "main": "simple-server.js",
  "scripts": {
    "start": "node simple-server.js",
    "dev": "nodemon simple-server.js",
    "test": "npm run test:cypress",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:playwright": "playwright test",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:ui": "playwright test --ui",
    "test:api": "playwright test tests/api/",
    "test:e2e": "cypress run",
    "test:all": "npm run test:cypress && npm run test:playwright",
    "setup": "npm install && npx playwright install",
    "clean": "rm -rf cypress/videos cypress/screenshots test-results playwright-report",
    "reset-db": "rm -f tenant_management.db"
  },
  "keywords": [
    "tenant-management",
    "full-stack",
    "express",
    "sqlite",
    "cypress",
    "playwright", 
    "e2e-testing",
    "api-testing",
    "ai-testing",
    "property-management",
    "test-automation"
  ],
  "author": "Darshini Karroo Tourmentin <darshini1101@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/darshinikt/TenantAITestGenerator.git"
  },
  "bugs": {
    "url": "https://github.com/darshinikt/TenantAITestGenerator/issues"
  },
  "homepage": "https://github.com/darshinikt/TenantAITestGenerator#readme",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "cypress": "^13.6.0",
    "@cypress/grep": "^4.0.1",
    "cypress-multi-reporters": "^1.6.4",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "openai": "^4.20.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "multer": "^1.4.5-lts.1"
  }
}
